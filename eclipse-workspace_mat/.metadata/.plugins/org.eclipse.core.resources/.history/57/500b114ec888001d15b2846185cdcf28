package com.main;

public class New {

	public static boolean isPrime(int num) {
		for (int j = 2; j < num; j++) {
			if (num % j == 0) {
				return false;
			}
		}
		return true;
	}
	
	public static void main(String[] args) {
		
//		int num = 50;
//		for (int i = 2; i <= num; i++) {
//			if (isPrime(i)) {
//				System.out.println(i);
//			}
//		}
		
		int sum=0;
		int rev;
		int num = 12345;
		
		while(num != 0) {
			rev = num%10;
			System.out.println(rev);
			num = num/10;
			rev = (sum *10) + rev;
		}
		System.out.println(sum);
		
	}
}
